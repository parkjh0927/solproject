	function getPopupSwiperHtml(popupData) {
		let popupUrl = popupData.LK_DIV_CD == 0 ? mainurl + "/detail/detail_view.html?cotid=" + popupData.LK_COT_ID : popupData.LK_DIV_CD == 1 ? popupData.LK_URL : '',
			popupHtml = '',
			now = new Date(),
			nowTime = now.getHours() * 60 + now.getMinutes(),
			obj = {
				hideSW: false,
				popupHtml: ""
			};

		if (popupData.EXPSR_TY_CD === "0") { // 시간/요일로 설정
			let weekdayArr = popupData.EXPSR_WKD_CD.split(""),
				sTimeArr = popupData.EXPSR_BGNG_TM.split(":"),
				eTimeArr = popupData.EXPSR_END_TM.split(":"),
				sTime = sTimeArr[0] * 60 + sTimeArr[1] * 1,
				eTime = eTimeArr[0] * 60 + eTimeArr[1] * 1,
				sw = false;
			for (let item of weekdayArr)
				if (item == now.getDay()) sw |= true;
			if (nowTime < sTime || nowTime > eTime || !sw) {
				obj.hideSW = true;
				return obj;
			}
		} else { // 기간으로 설정
			let sDate = new Date(popupData.EXPSR_BGNG_YMD),
				eDate = new Date(popupData.EXPSR_END_YMD);
			if (now < sDate || now > eDate.setDate(eDate.getDate() + 1)) {
				obj.hideSW = true;
				return obj;
			}
		}

		popupHtml += '<div class="swiper-slide">';

		if (popupUrl !== '') {
			popupHtml += '	<a href="' + popupUrl + '" target="_blank">';
			popupHtml += '		<img src="' + mainimgurl + popupData.POPUP_IMG_ID + '" alt="' + popupData.POPUP_DESC + '">';
			popupHtml += '	</a>';
		} else {
			popupHtml += '	<img src="' + mainimgurl + popupData.POPUP_IMG_ID + '" alt="' + popupData.POPUP_DESC + '">';
		}

		popupHtml += '</div>';
		obj.popupHtml = popupHtml;
		return obj;
	}

